{
  "name": "react-jizy",
  "description": "React component wrapper for lazy import",
  "version": "0.0.0",
  "types": "dist/jizy.d.ts",
  "main": "dist/jizy.js",
  "umd:main": "dist/jizy.umd.js",
  "module": "dist/jizy.m.js",
  "source": "src/jizy.ts",
  "repository": "git@github.com:nju33/jizy.git",
  "author": "nju33 <nju33.ki@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "component",
    "lazy",
    "import"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn demo:build && yarn src:build",
    "demo": "webpack-dev-server --config demo/webpack.config.js",
    "demo:build": "webpack --config demo/webpack.config.js",
    "prepare": "yarn build",
    "src:build": "microbundle -i src/jizy.ts --name Jizy",
    "test": "jest",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "@geekcojp/tslint-config": "^0.1.2",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.104",
    "@types/react": "^16.0.40",
    "@types/webpack": "^4.1.0",
    "awesome-typescript-loader": "^4.0.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.4.2",
    "lodash": "^4.17.5",
    "microbundle": "^0.4.4",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  }
}
